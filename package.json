  "scripts": {
    "api-prod": "node api/src/server.js",
    "test": "mocha --recursive api/tests",
    "hermione": "./node_modules/.bin/hermione hermione",
    "hermione-gui": "./node_modules/.bin/hermione gui hermione",

    "start-server": "nodemon ./dist/server.js",
    "build-server": "node ./dist/server.js",
    "watch-ssr": "webpack --config src/configs/server.webpack.config.js --watch",
    "watch-csr": "webpack --config src/configs/client.webpack.config.js --watch",
    "start-ssr": "concurrently --kill-others \"npm run watch-ssr \" \"npm run watch-csr\" \"npm run start-server\"",
    "start-csr": "webpack-dev-server --mode development --open --hot --config src/configs/client.webpack.config.js",
    "build-ssr": "webpack --config src/configs/server.webpack.config.js",
    "build-csr": "webpack --config src/configs/client.webpack.config.js",
    "build": "npm run build-csr && npm run build-ssr && npm run build-server"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
